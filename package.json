{
  "scripts": {
    "build": "ebam",
    "test": "npm run build && node test/code.js"
  },
  "name": "bitwane",
  "description": "A universal logger",
  "keywords": [
    "log",
    "logger",
    "print",
    "colors"
  ],
  "version": "1.8.1",
  "license": "MIT",
  "ebam": {
    "entry": "src/index.js",
    "transforms": {
      "dangerousForOf": false,
      "dangerousTaggedTemplateString": false
    },
    "test": {
      "dest": "test/code.js",
      "src": "test/src.js"
    }
  },
  "author": "Quentin Engles <hollowdoor99@gmail.com> (https://hollowdoor.github.io)",
  "main": "dist/bundle.js",
  "jsnext:main": "dist/bundle.es.js",
  "module": "dist/bundle.es.js",
  "dependencies": {
    "indent-string": "^3.2.0",
    "raw-object": "^1.0.0",
    "supports-color": "^5.1.0",
    "uni-compat": "^1.1.1"
  },
  "directories": {
    "test": "test"
  },
  "devDependencies": {},
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hollowdoor/bitwane.git"
  },
  "bugs": {
    "url": "https://github.com/hollowdoor/bitwane/issues"
  },
  "homepage": "https://github.com/hollowdoor/bitwane#readme"
}
